{
  "expo": {
    "name": "animalTesting",
    "slug": "animalTesting",
    "privacy": "public",
    "scheme": "animalTesting",
    "sdkVersion": "32.0.0",
    "icon": "./assets/playstore-icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/Logo_Animal_Testing.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/ItunesArtwork@2x.png",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "Le bon fonctionnement de l'application Animal Testing nécessite d'avoir un accès à votre appareil photo pour scanner les codes-barres des produits cosmétiques. Vous pouvez modifier cet autorisation dans les paramètres du téléphone, à la rubrique Applications.",
        "LSApplicationQueriesSchemes": [
          "fb",
          "fbapi",
          "fb-messenger-api",
          "fbauth2",
          "fbshareextension"
          "lyft", 
          "facebook", 
          "instagram;
        ]
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#000"
    },
    "android": {
      "icon": "./assets/playstore-icon.png"
    },
    "description": "Animal Testing App project."
  }
}
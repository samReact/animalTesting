{
  "expo": {
    "name": "Animal Testing",
    "slug": "animalTesting",
    "privacy": "public",
    "scheme": "animalTesting",
    "sdkVersion": "32.0.0",
    "icon": "./assets/animal-testing-icon_512x512.png",
    "platforms": ["ios", "android"],
    "version": "1.0.3",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.animaltesting.animaltesting",
      "icon": "./assets/ItunesArtwork@2x.png",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "Le bon fonctionnement de l'application Animal Testing nécessite d'avoir un accès à votre appareil photo pour scanner les codes-barres des produits cosmétiques. Vous pouvez modifier cet autorisation dans les paramètres du téléphone, à la rubrique Applications.",
        "LSApplicationQueriesSchemes": [
          "fb",
          "fbapi",
          "fb-messenger-api",
          "fbauth2",
          "fbshareextension",
          "facebook",
          "instagram"
        ]
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#000"
    },
    "android": {
      "package": "com.animaltesting.animaltesting",
      "adaptiveIcon": {
        "backgroundImage": "./assets/animal-testing-icon_512x512.png"
      }
    },
    "description": "Animal Testing App project."
  }
}

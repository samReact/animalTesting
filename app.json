{
  "expo": {
    "name": "Animal Testing",
    "slug": "animalTesting",
    "privacy": "public",
    "scheme": "animaltesting",
    "sdkVersion": "32.0.0",
    "icon": "./assets/animal-testing-icon_512x512.png",
    "platforms": ["ios", "android"],
    "version": "1.0.5",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "fr.animaltesting.ios",
      "icon": "./assets/animal-testing-icon_1024x1024.png",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "L'accès à votre appareil photo est nécessaire pour scanner les codes-barres des produits. Vous pouvez modifier cette autorisation dans les paramètres du téléphone, à la rubrique Applications.",
        "LSApplicationQueriesSchemes": [
          "fb",
          "fbapi",
          "fb-messenger-api",
          "fbauth2",
          "fbshareextension",
          "facebook",
          "instagram"
        ]
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#000"
    },
    "android": {
      "package": "fr.animaltesting.android",
      "adaptiveIcon": {
        "foregroundImage": "./assets/animaltesting_foreground_512x512.png",
        "backgroundColor": "#000000"
      }
    },
    "description": "Animal Testing App project."
  }
}
